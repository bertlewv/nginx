server {
    listen 80;
    listen [::]:80;
    server_name ms.rtswv.com *.ms.rtswv.com masonjarcatering.com *.masonjarcatering.com;

    location / {
                include proxy_params;
                proxy_pass http://localhost:9999;
        }
    access_log  /home/multisite/access.log;
    error_log   /home/multisite/error.log;
}

